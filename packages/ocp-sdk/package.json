{
  "name": "@onchainpoker/ocp-sdk",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./appchain": {
      "types": "./dist/appchain/index.d.ts",
      "default": "./dist/appchain/index.js"
    },
    "./evm": {
      "types": "./dist/evm/index.d.ts",
      "default": "./dist/evm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "test": "pnpm build && node --test test/*.test.js",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "ethers": "^6.16.0"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}

