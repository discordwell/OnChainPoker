{
  "name": "@onchainpoker/ocp-sdk",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./appchain": {
      "types": "./dist/appchain/index.d.ts",
      "default": "./dist/appchain/index.js"
    },
    "./cosmos": {
      "types": "./dist/cosmos/index.d.ts",
      "default": "./dist/cosmos/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "test": "pnpm build && node --test test/*.test.js",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.9.0",
    "@cosmjs/proto-signing": "^0.34.0",
    "@cosmjs/stargate": "^0.34.0",
    "ethers": "^6.16.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "ts-proto": "^2.7.7",
    "typescript": "^5.7.3"
  }
}
